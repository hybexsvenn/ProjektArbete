
<div class="input-group">
    <input ng-model="search" type="text" class="form-control" placeholder="Sök politiker...">
    <div class="input-group-btn" onload="Foo()">
        <button class="btn btn-default" type="submit">
            <i class="glyphicon glyphicon-search"></i>
        </button>
    </div>
</div>
<br /><br />
<ul style="list-style-type:none">
    <li ng-repeat="item in listOftodo | filter: search"><a asp-controller="Home" asp-action="Person">{{item}}</a></li>
</ul>



@section Scripts
{
    <script src="~/person.js"></script>
}















