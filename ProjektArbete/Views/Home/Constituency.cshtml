@{
    ViewBag.Title = "Valkrätts";
}
<div class="input-group">
    <input ng-change="change(search)" ng-model="search" type="text" class="form-control" placeholder="Sök valkrets...">
    <div class="input-group-btn">
        <button class="btn btn-default" type="submit">
            <i class="glyphicon glyphicon-search"></i>
        </button>
    </div>
</div>

<ul ng-show="searchBar" class="dropDownContent">
    <li ng-cloak ng-repeat="item in listOfSearch | filter: search track by $index">
        <p ng-click="selectedConstituency(item)">{{item}}</p>
    </li>
</ul>
<div id="content" ng-show="showSpecificConstituency">
    <div>
        <div id="divPersonCanvas">
            <canvas id="pieJChart" width="400" height="200"></canvas>
        </div>
        <div>
            <p><label for="date">Riksdagsår:</label><input ng-disabled="true" type="text" id="date" readonly style="border:0; color:#f6931f; font-weight:bold;"></p>
        </div>
        <div id="slider-range"></div>
    </div>
</div>

<br /><br />

@section scripts
    {
    <script src="~/Constituency.js"></script>
}
